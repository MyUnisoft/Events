{
  "metadata": {
    "type": "object",
    "properties": {
      "agent": {
        "type": "string"
      },
      "origin": {
        "type": "object",
        "properties": {
          "endpoint": {
            "type": "string"
          },
          "method": {
            "type": "string",
            "enum": ["POST", "PATCH", "PUT", "DELETE"]
          },
          "requestId": {
            "type": "string",
            "pattern": "req-[A-Z,a-z,0-9]+"
          }
        },
        "required": ["endpoint", "method"],
        "additionalProperties": false
      },
      "createdAt": {
        "type": "number"
      }
    },
    "required": ["agent", "createdAt"],
    "additionalProperties": false
  }
}

