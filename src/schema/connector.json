{
  "connector": {
    "create": {
      "type": "object",
      "properties": {
        "connectorId": {
          "type": "string",
          "pattern": "^[0-9]+"
        }
      },
      "required": ["connectorId"],
      "additionalProperties": false
    },
    "update": {
      "type": "object",
      "properties": {
        "connectorId": {
          "type": "string",
          "pattern": "^[0-9]+"
        }
      },
      "required": ["connectorId"],
      "additionalProperties": false
    },
    "delete": {
      "type": "object",
      "properties": {
        "ids": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[0-9]+"
          },
          "uniqueItems": true,
          "minItems": 1
        }
      },
      "required": ["ids"],
      "additionalProperties": false
    }
  }
}
